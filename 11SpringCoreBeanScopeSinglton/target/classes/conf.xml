<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->


	<bean id="db" class="spring.core.bean.DbInfo" scope="singleton">

		<property name="username" value="root"></property>
		<property name="password" value="Sangam@123"></property>

	</bean>


	<bean id="sinfo" class="spring.core.bean.StudentInfo" scope="prototype">

		<property name="itsName" value="Sangam"></property>
		<property name="itsLastName" value="Patil"></property>

	</bean>
</beans>
