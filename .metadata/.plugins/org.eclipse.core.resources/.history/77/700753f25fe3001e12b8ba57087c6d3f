package spring.core.tickets;

import java.util.Random;

public class Ticket {
	
	private int ticketNum =0;
	
	

	public Ticket() {
		
		ticketNum = Random.nextInt(9999);
		
	}

	public int getTicketNum() {
		return ticketNum;
	}

	public void setTicketNum(int ticketNum) {
		this.ticketNum = ticketNum;
	}
	
	
	

}
